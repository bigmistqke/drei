    "rollup-plugin-terser": "^7.0.2",
    "semantic-release": "^21.0.6",
    "serve": "^14.2.0",
    "solid-js": "^1.7.8",
    "storybook": "^7.1.1",
    "storybook-solidjs": "^1.0.0-beta.2",
    "storybook-solidjs-vite": "^1.0.0-beta.2",
